{
  "name": "saas",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.js --watch ./server.js --watch ./build",
    "dev:remix": "remix dev",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "postinstall": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:db:push": "prisma db push",
    "format": "prettier --write .",
    "localtunnel": "lt --port 3000",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc -b",
    "test:e2e:dev": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "validate": "run-p lint typecheck test:e2e:run",
    "checkUpdates": "npx npm-check-updates"
  },
